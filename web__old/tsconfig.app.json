{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.d.ts",
    "src/types/**/*.d.ts"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "types": [
      "node"
    ],
    "module": "esnext",
    "composite": true,
    // "esModuleInterop": true,
    // "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*",
      ],
      "~/*": [
        "src/*"
      ]
    },
    "lib": [
      "ESNext",
      "DOM",
      "esnext.promise"
    ]
  }
}